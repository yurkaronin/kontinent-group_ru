$(function () {
	//Если мы на стр. акции и новости, то инициализируем аякс-фильтрацию
	if ($('.js-san-category-switch').length > 0) {

		var san_lock_refresh_project = false;

		function ajaxChangeSaNcontent() {
			var targetContainer = $('.js-news-list'); //  Контейнер, в котором хранятся элементы

			var u = new Url();
			u.query['section_id'] = $('.js-san-category-switch.tabs-controls__item--active').attr('data-id');
			var project_id = $('.js-san-project-select').val();

			if (project_id === 'false' && typeof (u.query.project_id) !== 'undefined') {
				delete u.query.project_id;
			} else {
				u.query.project_id = project_id;
			}

			var url = u.toString();

			$.ajax({
				type: 'GET',
				url: url,
				dataType: 'html',
				success: function (data) {
					//  Удаляем старую навигацию
					$('.js-news-loadmore').remove();
					var JQdata = $(data);
					var elements = JQdata.find('.js-news-item').addClass('loadmore-item-temp-hidden'),  //  Ищем элементы
						pagination = JQdata.find('.js-news-loadmore');//  Ищем навигацию
					elements.find('img').removeAttr('loading');
					targetContainer.html(elements);   //  Добавляем посты в конец контейнера
					setTimeout(function () {
						elements.removeClass('loadmore-item-temp-hidden');
					}, 300);
					$('.js-news-loadmore-wrap').append(pagination); //  добавляем навигацию следом

					san_lock_refresh_project = true;
					$('.js-san-project-select .js-option-all').removeData('data').html(JQdata.find('.js-san-project-select .js-option-all').html());
					$('.js-san-project-select').removeData('data').trigger("change");
					san_lock_refresh_project = false;
				}
			});
		};
		$('.js-san-category-switch').on("click", function () {
			$(this).addClass('tabs-controls__item--active').siblings('.tabs-controls__item--active').removeClass('tabs-controls__item--active');
			ajaxChangeSaNcontent();
		});
		$('.js-san-project-select').on("change", function () {
			if (!san_lock_refresh_project) {
				ajaxChangeSaNcontent();
			}
		});
	}


	$('body').on('keyup change', '.js-need-validate', function () {
		validateField($(this));
	});


	$('body').on('click', '.js-pw-bank__btn', function (e) {
		e.preventDefault();
		e.stopPropagation();

		var modal = $('#call-me-modal');
		var btn = $(this);
		var bank = btn.closest('.js-bank');
		var bank_name = bank.find('.pw-bank__name').html();
		var section = btn.closest('section');
		var subject = "Запрос на расчёт ипотеки || Ипотечная программа: " + section.find('.js-pw-select-program option:selected').html() + " || Стоимость недвижимости: " + section.find('.js-pw-input-cost').val() + " || Первоначальный взнос: " + section.find('.js-pw-input-firstPayment').val() + " || Срок: " + section.find('.js-pw-input-term').val() + " || Банк: " + bank_name;

		var title = 'Рассчитать ипотеку';
		var dataTitle = btn.attr('data-title');
		if (isdef(dataTitle) && dataTitle.length > 0) {
			title = dataTitle;
		}
		if (isdef(dataTitle) && dataTitle === 'btn') {
			title = btn.text();
		}
		window.prevModalTitle = title;
		var dataSubject = btn.attr('data-subject');
		if (isdef(dataSubject) && dataSubject.length > 0) {
			subject = dataSubject;
		}

		modal.attr('data-custom-title', title);
		modal.find('.simple-modal__title').html(title);

		modal.find('.js-webform-field-subject').val(subject);
		modal.find('.js-webform-field-source').val(document.location.href);

		Fancybox.show([{src: "#call-me-modal", type: "inline", closeButton: false}]);
	});


	if ($('.js-reset-resetable-filters').length > 0) {
		$('body').on('click', '.js-reset-resetable-filters', function () {
			$('.js-resetable-filters-select').each(function () {
				$(this).val('all').trigger('change');
			});
			$('.js-resetable-filters-inp-min').each(function () {
				$(this).val($(this).closest('.js-range-slider').attr('data-min'))[0].dispatchEvent(new Event("change"));
			});
			$('.js-resetable-filters-inp-max').each(function () {
				$(this).val($(this).closest('.js-range-slider').attr('data-max'))[0].dispatchEvent(new Event("change"));
			});
			$('.js-resetable-filters-chb').prop('checked', false);
			setTimeout(function(){
				var preparedValues = prepareFiltersVals($('.js-filters-row'));
				debouncedRefreshFlatsCount(preparedValues);
			},300)

		});


	}

	$('.js-filters-row select, .js-filters-row input').on('change rsChange', function () {
		var filtersBlock = $(this).closest('.js-filters-row');
		var preparedValues = prepareFiltersVals(filtersBlock);

		if (typeof (preparedValues.letters) !== 'undefinded' && preparedValues.letters !== 'all' && preparedValues.letters.length === 0) {
			//Задан несовместимый набор селектов(фильтров, фильтрующих литеры). Пересечение их списка id пустое множество. Еол-во найденных кввартир обязательно 0
			$('.js-filter-result-items-count').html('0 квартир');
		} else {
			debouncedRefreshFlatsCount(preparedValues);
		}
	});


	$(document).on('click', '.js-apply-filter-flats', function (e) {
		e.preventDefault();

		if ($('#flats-page-main-section').length === 0) {//мы не на странице подбора квартир
			saveFiltersVals();
			var url = new Url(window.flatsUrl);
			url.query['needLoadParams'] = "true";
			document.location.href = url;
		}
	});





	$(document).on('click', '.js-set-filters-from-btn-and-go', function (e) {
		e.preventDefault();
		var btn=$(this);
		var selectProjVal=btn.attr('data-select-project');
		var roomsChbVal=btn.attr('data-rooms-chb');
		var areaMinVal=parseInt(btn.attr('data-area-min'));
		var areaMaxVal=parseInt(btn.attr('data-area-max'));

		var preparedValues={
			"select[name=\"project\"]": "all",
			"select[name=\"deadline\"]": "all",
			"select[name=\"class\"]": "all",
			"select[name=\"finishing\"]": "all",
			"select[name=\"technology\"]": "all",
			"rooms_count": [],
			"price_min": "0 ₽",
			"price_max": "9 999 000 000 ₽",
			"area_min": "0 м²",
			"area_max": "999 м²",
			"kitchen_area_min": "0 м²",
			"kitchen_area_max": "999 м²",
			"floor_min": "0 этаж",
			"floor_max": "99 этаж"
		};

		if(typeof(selectProjVal)!=="undefined" && selectProjVal.length>0){
			preparedValues["select[name=\"project\"]"]=selectProjVal;
		}

		//Предполагается выбор таким методом только одного варианта. Более не нужно
		if(typeof(roomsChbVal)!=="undefined" && roomsChbVal.length>0){
			preparedValues["rooms_count"]=[roomsChbVal];
		}

		if(!isNaN(areaMinVal) ?? areaMinVal>0){
			preparedValues["area_min"]=areaMinVal+" м²";
		}
		if(!isNaN(areaMaxVal) ?? areaMaxVal>0){
			preparedValues["area_max"]=areaMaxVal+" м²";
		}
		localStorage.setItem('savedFiltersValues', JSON.stringify(preparedValues));

		var url = new Url(window.flatsUrl);
		url.query['needLoadParams'] = "true";
		document.location.href = url;
	});


	if ($('#flats-page-main-section').length > 0) {//мы на странице подбора квартир
		var url = new Url();
		if (url.query['needLoadParams'] === "true") {
			loadFiltersVals();
			setTimeout(function () {
				//window.disabled_scroll_to_flats_after_filter=true;
				$('.js-apply-filter-flats').first().trigger('click');
			}, 300);
		}
	}



	//Если уже есть выбранные параметры фильтра, то применяем их
	let roomsCheckbox = $('.js-flats-filter input[name="rooms_count"]:checked');
	if (roomsCheckbox != null && roomsCheckbox && roomsCheckbox.length > 0) {
		let preparedValues = prepareFiltersVals($('.js-filters-row'));
		debouncedRefreshFlatsCount(preparedValues);
		setTimeout(function () {
			$('.js-apply-filter-flats').first().trigger('click');
		}, 300);
	}
});


function prepareFiltersVals(filtersBlock) {
	var preparedFilters = {};

	var rooms_count_chb = filtersBlock.find('input[type="checkbox"][name="rooms_count"]:checked');
	if (rooms_count_chb.length > 0) {
		preparedFilters.rooms_types = jQuery.map(rooms_count_chb, function (el, i) {
			return $(el).val();
		});
	}

	var selectsForLettersValues = [
		filtersBlock.find('select[name="project"]').val(),
		filtersBlock.find('select[name="deadline"]').val(),
		filtersBlock.find('select[name="class"]').val(),
		filtersBlock.find('select[name="finishing"]').val(),
		filtersBlock.find('select[name="technology"]').val(),
	];
	var sflNotAllValues = [];

	selectsForLettersValues.forEach(function (val) {
		if (val !== 'all') {
			sflNotAllValues.push(val.split(','));
		}
	});
	var lettersIdsIntersection;
	if (sflNotAllValues.length > 0) {
		sflNotAllValues.forEach(function (val, idx) {
			if (idx === 0) {
				//закинем значение
				lettersIdsIntersection = val;
			} else {
				//получим пересечение массивов
				lettersIdsIntersection = lettersIdsIntersection.filter(function (n) {
					return val.indexOf(n) !== -1;
				});
			}
		});

		preparedFilters.letters = lettersIdsIntersection;
	} else {
		preparedFilters.letters = 'all';
	}

	preparedFilters.price_min = getProcessedRSinpVal(filtersBlock.find('input[name="price_min"]'), 'min');
	preparedFilters.price_max = getProcessedRSinpVal(filtersBlock.find('input[name="price_max"]'), 'max');
	preparedFilters.area_min = getProcessedRSinpVal(filtersBlock.find('input[name="area_min"]'), 'min');
	preparedFilters.area_max = getProcessedRSinpVal(filtersBlock.find('input[name="area_max"]'), 'max');
	preparedFilters.kitchen_area_min = getProcessedRSinpVal(filtersBlock.find('input[name="kitchen_area_min"]'), 'min');
	preparedFilters.kitchen_area_max = getProcessedRSinpVal(filtersBlock.find('input[name="kitchen_area_max"]'), 'max');
	preparedFilters.floor_min = getProcessedRSinpVal(filtersBlock.find('input[name="floor_min"]'), 'min');
	preparedFilters.floor_max = getProcessedRSinpVal(filtersBlock.find('input[name="floor_max"]'), 'max');

	return preparedFilters;
}


function saveFiltersVals() {
	var filtersBlock = $('.js-filters-row');
	var preparedFilters = {};
	preparedFilters['select[name="project"]'] = filtersBlock.find('select[name="project"]').val();
	preparedFilters['select[name="deadline"]'] = filtersBlock.find('select[name="deadline"]').val();
	preparedFilters['select[name="class"]'] = filtersBlock.find('select[name="class"]').val();
	preparedFilters['select[name="finishing"]'] = filtersBlock.find('select[name="finishing"]').val();
	preparedFilters['select[name="technology"]'] = filtersBlock.find('select[name="technology"]').val();

	var checked = [];
	filtersBlock.find('input[type="checkbox"][name="rooms_count"]:checked').each(function () {
		checked.push($(this).val());
	});

	preparedFilters['rooms_count'] = checked;

	preparedFilters.price_min = filtersBlock.find('input[name="price_min"]').val();
	preparedFilters.price_max = filtersBlock.find('input[name="price_max"]').val();
	preparedFilters.area_min = filtersBlock.find('input[name="area_min"]').val();
	preparedFilters.area_max = filtersBlock.find('input[name="area_max"]').val();
	preparedFilters.kitchen_area_min = filtersBlock.find('input[name="kitchen_area_min"]').val();
	preparedFilters.kitchen_area_max = filtersBlock.find('input[name="kitchen_area_max"]').val();
	preparedFilters.floor_min = filtersBlock.find('input[name="floor_min"]').val();
	preparedFilters.floor_max = filtersBlock.find('input[name="floor_max"]').val();
	localStorage.setItem('savedFiltersValues', JSON.stringify(preparedFilters));
	//return preparedFilters;

}


function loadFiltersVals() {
	var filtersBlock = $('.js-filters-row');
	var preparedFilters = localStorage.getItem('savedFiltersValues');
	if (typeof (preparedFilters) === 'undefined') return;
	preparedFilters = JSON.parse(preparedFilters);
	if (typeof (preparedFilters) === 'undefined') return;

	filtersBlock.find('select[name="project"]').val(preparedFilters['select[name="project"]']);
	filtersBlock.find('select[name="deadline"]').val(preparedFilters['select[name="deadline"]']);
	filtersBlock.find('select[name="class"]').val(preparedFilters['select[name="class"]']);
	filtersBlock.find('select[name="finishing"]').val(preparedFilters['select[name="finishing"]']);
	filtersBlock.find('select[name="technology"]').val(preparedFilters['select[name="technology"]']);
	delete preparedFilters['select[name="project"]'];
	delete preparedFilters['select[name="deadline"]'];
	delete preparedFilters['select[name="class"]'];
	delete preparedFilters['select[name="finishing"]'];
	delete preparedFilters['select[name="technology"]'];

	var checked = [];
	filtersBlock.find('input[type="checkbox"][name="rooms_count"]').each(function () {
		if (preparedFilters['rooms_count'].includes($(this).val())) {
			$(this).prop('checked', true);
		}
	});
	delete preparedFilters['rooms_count'];


	for (var key in preparedFilters) {
		if (preparedFilters.hasOwnProperty(key)) {
			filtersBlock.find('input[name="' + key + '"]').val(preparedFilters[key]);
		}
	}

	filtersBlock.find('select').trigger('change');
	filtersBlock.find('input').trigger('changeBoth');
	localStorage.removeItem('savedFiltersValues')
}


function refreshFlatsCount(preparedValues) {

	$.ajax({
		type: 'POST',
		url: '/local/ajax/refresh_results_count.php',
		data: preparedValues,
		dataType: 'html',
		success: function (data) {
			var count = parseInt(data);
			$('.js-filter-result-items-count').html(count + ' ' + num_word(count, ['квартира', 'квартиры', 'квартир']));

			$('.js-filter-result-items-count-alt').html(count + ' ' + num_word(count, ['квартир', 'квартир', 'квартир']));
		}
	});
}

debouncedRefreshFlatsCount = debounce(refreshFlatsCount, 200);

function getProcessedRSinpVal(input, type) {
	//нужно только для целых чисел. Поэтому для float логику не пишу
	var rs = input.closest('.js-range-slider');
	var rsMin = parseInt(rs.attr('data-min'));
	var rsMax = parseInt(rs.attr('data-max'));
	var val = parseInt(input.val().replaceAll(/\D/g, ""));

	switch (type) {
		case 'min':
			if (val !== rsMin) {
				return val;
			} else {
				return -1;
			}
			break;
		case 'max':
			if (val !== rsMax) {
				return val;
			} else {
				return -1;
			}
			break;
		default:
			return -2;
			break;
	}
}



$('body').on('click','.js-galcard-fancybox-trigger',function(){
	console.log($(this).closest('.gallery-card').find('[data-fancybox]'));
	$(this).closest('.gallery-card').find('[data-fancybox]').first()[0].click();
});

function popupSuccess() {
	Fancybox.close();
	Fancybox.show([{src: "#success-modal", type: "inline", closeButton: false}]);
};


function validateField($this) {
	var reg_email = /.+@.+/;
	currentValid = true;

	switch ($this.attr('data-validate-type')) {
		case 'checked':
			if ($this.is(':checked')) {
			} else {
				currentValid = false;
			}
			break;
		case 'non-empty':
			if ($this.val().length > 0) {
			} else {
				currentValid = false;
			}
			break;
		case 'email':
			$this.val($.trim($this.val()));
			if ($this.val().length > 0 && $this.val().match(reg_email)) {
			} else {
				currentValid = false;
			}
			break;
		case 'phone':
			if (typeof ($this[0].inputmask) !== 'undefined') {
				if (!($this[0].inputmask.isComplete())) {
					currentValid = false;
				}
			} else {
				$this.val($.trim($this.val()));
				if ($this.val().length > 0) {
				} else {
					currentValid = false;
				}
			}
			break;
		default:
			break;
	}
	if (currentValid) {
		$this.closest('.js-input-wrap').removeClass('input-wrap-invalid');

	} else {
		$this.closest('.js-input-wrap').addClass('input-wrap-invalid');
		valid = false;
	}
	return currentValid;
}
/* End */
